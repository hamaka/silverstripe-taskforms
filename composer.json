{
    "name": "hamaka/silverstripe-taskforms",
    "description": "Utility to make Silverstripe tasks more interactive and better looking.\n\n",
    "type": "silverstripe-vendormodule",
    "homepage": "https://github.com/hamaka/silverstripe-taskforms",
    "keywords": [
        "silverstripe",
        "buildtask",
        "task",
        "taskrunner",
        "form",
        "hamaka"
    ],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Sander van Scheepen",
            "email": "sander@hamaka.nl",
            "role": "Developer"
        },
        {
            "name": "Carlo Riedstra",
            "email": "carlo@hamaka.nl",
            "role": "Developer"
        },
        {
            "name": "Bauke Zwaan",
            "email": "bauke@hamaka.nl",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.1",
        "silverstripe/framework": "^4 | ^5"
    },
    "autoload": {
        "psr-4": {
            "hamaka\\silverstripe-taskforms\\": "src/"
        }
    },
    "support": {
        "issues": "https://github.com/hamaka/silverstripe-taskforms/issues"
    },
    "extra": {
        "installer-name": "Silverstripe Taskforms",
        "branch-alias": {
            "dev-master": "2.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
